      context: C:\Users\Lenovo\Desktop\CSharp项目\就业项目\Plaza
      dockerfile: Plaza.Net.MVCAdmin/Dockerfile
    container_name: mvcadmin-container
    depends_on:
      sqlserver:
        condition: service_started
